---
layout: default
---
<article class="post" itemscope itemtype="http://schema.org/BlogPosting">
    <header>
        <h1><a href="{{ page.url }}">{{ page.title }}</a></h1>
        <time datetime="{{ page.date }}">{{ page.date | date_to_string }}</time>
    </header>
    <p>{{ content }}</p>
</article>